{
  "name": "angularstarter",
  "version": "0.0.0",
  "description": "basic set-up for angularjs 1.5.x, angular-ui-router, and angular-material",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "typings install",
    "poststart": "npm update",
    "start": "cd app && live-server --no-browser --mount='/node_modules':'../node_modules' --mount='/built':'../built' --mount='/app':'../app'"
  },
  "engines": {
    "node": "5.1.0",
    "npm": "3.4.1"
  },
  "keywords": [
    "angularjs",
    "angular",
    "Material",
    "ui-router"
  ],
  "author": "joseph phillips",
  "license": "MIT",
  "devDependencies": {
    "live-server": "1.0.0",
    "typings": "1.3.2",
    "typescript": "1.8.10"
  },
  "dependencies": {
    "jquery": "3.1.0",
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
    "angular-aria": "1.5.8",
    "angular-material": "1.1.0-rc.5",
    "angular-messages": "1.5.8",
    "angular-route": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-ui-router": "^0.3.1",
    "toastr": "^2.1.2",
    "express": "4.13.3"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}